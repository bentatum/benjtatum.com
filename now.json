{
  "version": 2,
  "name": "benjtatum.com",
  "alias": "benjtatum.com",
  "builds": [
    {
      "src": "www/package.json",
      "use": "https://github.com/ajbouh/now-sapper",
      "config": {
        "runtime": "nodejs12.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ]
}